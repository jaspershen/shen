file = {:C$\backslash$:/Users/MI/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Mattsson et al. - 2012 - Longitudinal cerebrospinal fluid biomarkers over four years in mild cognitive impairment.pdf:pdf},
isbn = {1875-8908},
issn = {1875-8908},
journal = {J. Alzheimers. Dis.},
