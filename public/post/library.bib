issn = {0022-2275},
journal = {J. Lipid Res.},
keywords = {Aged,Alzheimer Disease/ blood,Biological Markers/ blood,Case-Control Studies,Cholesterol/blood,Chromatography, Liquid,Desmosterol/blood,Female,Humans,Male,Mass Spectrometry,Metabolomics/ methods,Sex Factors},
number = {3},
