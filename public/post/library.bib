keywords = {UPLC-Q-TOF-MS/MS,biomarker,metabonomics,nutritional rickets,urine},
number = {9},
pages = {4131--4142},
pmid = {25051233},
title = {{Biomarkers identified by urinary metabonomics for noninvasive diagnosis of nutritional rickets}},
