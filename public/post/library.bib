file = {:C$\backslash$:/Users/MI/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Ansoleaga et al. - 2015 - Deregulation of purine metabolism in Alzheimer's disease.pdf:pdf},
issn = {1558-1497},
journal = {Neurobiol. Aging},
keywords = {Alzheimer's disease,Entorhinal cortex,Frontal cortex,Mass spectrometry,Metabolomics,Precuneus,Purine metabolism},
number = {1},
pages = {68--80},
pmid = {25311278},
