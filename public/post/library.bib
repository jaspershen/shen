@article{Akhmedov2017,
abstract = {With the recent technological developments a vast amount of
high-throughput data has been profiled to understand the mechanism of
complex diseases. The current bioinformatics challenge is to interpret
the data and underlying biology, where efficient algorithms for
analyzing heterogeneous high-throughput data using biological networks
are becoming increasingly valuable. In this paper, we propose a software
package based on the Prize-collecting Steiner Forest graph optimization
approach. The PCSF package performs fast and user-friendly network
analysis of high-throughput data by mapping the data onto a biological
networks such as protein-protein interaction, gene-gene interaction or
any other correlation or coexpression based networks. Using the
