keywords = {alzheimer,hypoxia,lipidomics,metabolomics,mild cognitive impairment,pentose phosphate pathway,s disease},
number = {12},
pages = {e57},
pmid = {22832349},
title = {{Metabolome in progression to Alzheimer's disease}},
volume = {1},
year = {2011}
}
