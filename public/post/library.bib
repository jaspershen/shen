pages = {1575--1584},
title = {{Intestinal Microbial Metabolism of Phosphatidylcholine and Cardiovascular Risk}},
url = {http://www.nejm.org/doi/10.1056/NEJMoa1109400},
volume = {368},
year = {2013}
}
