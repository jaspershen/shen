author = {Neuweger, Heiko and Albaum, Stefan P. and Dondrup, Michael and Persicke, Marcus and Watt, Tony and Niehaus, Karsten and Stoye, Jens and Goesmann, Alexander},
doi = {10.1093/bioinformatics/btn452},
file = {:C$\backslash$:/Users/MI/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Neuweger et al. - 2008 - MeltDB A software platform for the analysis and integration of metabolomics experiment data.pdf:pdf},
isbn = {1367-4803},
issn = {13674803},
journal = {Bioinformatics},
