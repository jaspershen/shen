keywords = {Algorithm,Correlation,Cross-platform normalization,Desorption electrospray ionization,Desorption ionization mass spectrometry,Laser desorption ionization,Lipids,Mass spectrometry,Peak annotation,Principal component analysis,Rapid evaporative ionization mass spectrometry,k-Nearest neighbor},
