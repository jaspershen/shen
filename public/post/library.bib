issn = {01974580},
journal = {Neurobiol. Aging},
keywords = {Alzheimer's disease,Biomarker,Ceramides,Memory,Serum lipid markers,Sphingomyelins},
pages = {17--24},
pmid = {18455839},
title = {{Serum sphingomyelins and ceramides are early predictors of memory impairment}},
