issn = {1535-3893},
journal = {J. Proteome Res.},
pages = {150326155042009},
pmid = {25784130},
title = {{Discovery and Validation of Plasma Biomarkers for Major Depressive Disorder Classification Based on Liquid Chromatography–Mass Spectrometry}},
url = {http://pubs.acs.org/doi/abs/10.1021/acs.jproteome.5b00144},
