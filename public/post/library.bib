keywords = {Deep convolutional neural networks,Deep learning,Drink detection,Drink recognition,Food detection,Food recognition,NutriNet,Parkinson's disease},
number = {7},
pages = {1--19},
title = {{Nutrinet: A deep learning food and drink image recognition system for dietary assessment}},
