journal = {J. Am. Geriatr. Soc.},
keywords = {Alzheimer's disease,Cerebrospinal fluid,FDG-PET,MRI,Mild cognitive impairment,Neuropsychological assessment},
number = {9},
pages = {1705--1710},
pmid = {21883100},
title = {{Value of neuropsychological tests, neuroimaging, and biomarkers for diagnosing Alzheimer's disease in younger and older age cohorts}},
volume = {59},
year = {2011}
}
